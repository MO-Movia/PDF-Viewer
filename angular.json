{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "mo-pdf-viewer": {
      "projectType": "library",
      "root": "projects/mo-pdf-viewer",
      "sourceRoot": "projects/mo-pdf-viewer/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/mo-pdf-viewer/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/mo-pdf-viewer/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/mo-pdf-viewer/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/mo-pdf-viewer/tsconfig.spec.json",
            "karmaConfig": "projects/mo-pdf-viewer/karma.conf.js",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          },
          "configurations": {
            "jenkins": {
              "browsers": "ChromeHeadless",
              "codeCoverage": true,
              "progress": false,
              "reporters": [
                "junit"
              ],
              "watch": false
            }
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/mo-pdf-viewer/**/*.ts",
              "projects/mo-pdf-viewer/**/*.html"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "schematicCollections": [
      "@angular-eslint/schematics"
    ]
  }
}
